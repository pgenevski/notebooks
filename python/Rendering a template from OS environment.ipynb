    "from jinja2 import Template\n",
    "import os\n",
    "\n",
    "tpl = '''PAGER: {{ PAGER }}\\nPAGER1: {{PAGER1 }}'''\n",
    "template = Template(tpl)\n",
    "\n",
    "defaults = {'PAGER':'stuff','PAGER1':'stuff1'}\n",
    "data = {**defaults, **os.environ}  # fill in default values\n",
    "out = template.render(data)\n",
    "\n",
    "print(out)"
   ]
